{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "string",
            "defaultValue": "{{ oms_location }}"
        },
        "resourcegroup": {
            "type": "string",
            "defaultValue": "{{ oms_resourceGroup }}"
        },
        "subscriptionId": {
            "type": "string",
            "defaultValue": "{{ oms_subscription }}"
        },
        "workspace": {
            "type": "string",
            "defaultValue": "{{ oms_workspaceName }}"
        },
        "workspaceapiversion": {
            "type": "string",
            "defaultValue": "2015-11-01-preview"
        }
    },
    "resources": [
        {
            "apiVersion": "[parameters('workspaceapiversion')]",
            "name": "[parameters('workspace')]",
            "type": "Microsoft.OperationalInsights/workspaces",
            "location": "[parameters('location')]",
            "id": "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'))]",
            "resources": [
                {
                    "apiVersion": "2015-11-01-preview",
                    "name": "Server Performance",
                    "type": "views",
                    "location": "[parameters('location')]",
                    "id": "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'),'/views/Server Performance')]",
                    "dependson": [
                        "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'))]"
                    ],
                    "properties": {
                        "Id": "Server Performance",
                        "Name": "Server Performance",
                        "Author": "cameron.fuller@catapultsystems.com",
                        "Source": "Local",
                        "Dashboard": [
                            {
                                "Id": "TwoNumberTileListBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Data sent by Computer",
                                        "newGroup": true,
                                        "icon": "data:image/jpeg;base64,",
                                        "useIcon": false
                                    },
                                    "Tile": {
                                        "Legend": "Computers sending data",
                                        "Query": "MG:* | Measure Max(TimeGenerated) as LastData by Computer | where LastData>NOW-1HOURS"
                                    },
                                    "SecondTile": {
                                        "Legend": "Computers not sending data",
                                        "Query": "MG:* | Measure Max(TimeGenerated) as LastData by Computer | where LastData<NOW-1HOURS"
                                    },
                                    "List": {
                                        "Query": "* | measure count() by Computer",
                                        "HideGraph": false,
                                        "enableSparklines": true,
                                        "operation": "Summary",
                                        "ColumnsTitle": {
                                            "Name": "Computer",
                                            "Value": "Data Sent to OMS"
                                        },
                                        "Color": "#0072c6",
                                        "thresholds": {
                                            "isEnabled": false,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": "60",
                                                    "color": "#fcd116",
                                                    "isDefault": false
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": "90",
                                                    "color": "#ba141a",
                                                    "isDefault": false
                                                }
                                            ]
                                        },
                                        "NameDSVSeparator": "",
                                        "NavigationQuery": "{selected item}"
                                    }
                                }
                            },
                            {
                                "Id": "LineChartBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Processor Utilization",
                                        "newGroup": true,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Header": {
                                        "Title": "Processor over time",
                                        "Subtitle": ""
                                    },
                                    "LineChart": {
                                        "Query": "Type:Perf (ObjectName:Processor OR ObjectName:System) AND (CounterName=\"% Processor Time\") | measure avg(CounterValue) by Computer",
                                        "yAxis": {
                                            "isLogarithmic": false,
                                            "units": {
                                                "baseUnitType": "",
                                                "baseUnit": "",
                                                "displayUnit": ""
                                            },
                                            "customLabel": ""
                                        }
                                    },
                                    "List": {
                                        "Query": "Type:Perf (ObjectName:Processor OR ObjectName:System) AND (CounterName=\"% Processor Time\") | measure percentile90(CounterValue) by Computer",
                                        "HideGraph": false,
                                        "enableSparklines": true,
                                        "operation": "Summary",
                                        "ColumnsTitle": {
                                            "Name": "Computer",
                                            "Value": "% Utilization"
                                        },
                                        "Color": "#0072c6",
                                        "thresholds": {
                                            "isEnabled": true,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": "90",
                                                    "color": "#fcd116",
                                                    "isDefault": false
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": "95",
                                                    "color": "#ba141a",
                                                    "isDefault": false
                                                }
                                            ]
                                        },
                                        "NameDSVSeparator": "",
                                        "NavigationQuery": "Type:Perf (ObjectName:Processor OR ObjectName:System) AND (CounterName=\"% Processor Time\") | measure percentile90(CounterValue) by Computer | Display StackedBarChart"
                                    }
                                }
                            },
                            {
                                "Id": "LineChartBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Disk Usage",
                                        "newGroup": true,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Header": {
                                        "Title": "Free Megabytes",
                                        "Subtitle": ""
                                    },
                                    "LineChart": {
                                        "Query": "Type:Perf  ((ObjectName:LogicalDisk AND CounterName:\"% Free Space\" )) | Measure avg(CounterValue) by Computer",
                                        "yAxis": {
                                            "isLogarithmic": false,
                                            "units": {
                                                "baseUnitType": "",
                                                "baseUnit": "",
                                                "displayUnit": ""
                                            },
                                            "customLabel": ""
                                        }
                                    },
                                    "List": {
                                        "Query": "Type:Perf  ((ObjectName:LogicalDisk AND CounterName:\"% Free Space\" AND  InstanceName<>\"_Total\")) | measure percentile90(CounterValue) as AvgFreeSpace by Computer,InstanceName | sort AvgFreeSpace",
                                        "HideGraph": false,
                                        "enableSparklines": false,
                                        "operation": "Summary",
                                        "ColumnsTitle": {
                                            "Name": "Computer",
                                            "Value": "Free Space"
                                        },
                                        "Color": "#0072c6",
                                        "thresholds": {
                                            "isEnabled": true,
                                            "values": [
                                                {
                                                    "name": "Error",
                                                    "threshold": "Default",
                                                    "color": "#ba141a",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": "10",
                                                    "color": "#fcd116",
                                                    "isDefault": false
                                                },
                                                {
                                                    "name": "Healthy",
                                                    "threshold": "20",
                                                    "color": "#007233",
                                                    "isDefault": false
                                                }
                                            ]
                                        },
                                        "NameDSVSeparator": "",
                                        "NavigationQuery": "Type:Perf  ((ObjectName:LogicalDisk AND CounterName:\"% Free Space\" AND  InstanceName!=\"_Total\")) | measure percentile90(CounterValue) as AvgFreeSpace by Computer,InstanceName | sort AvgFreeSpace | display StackedBarChart"
                                    }
                                }
                            },
                            {
                                "Id": "LineChartBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Memory",
                                        "newGroup": true,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Header": {
                                        "Title": "% Committed Bytes in Use",
                                        "Subtitle": ""
                                    },
                                    "LineChart": {
                                        "Query": "Type:Perf (ObjectName: Memory) AND (CounterName=\"% Committed Bytes In Use\") | Measure percentile90(CounterValue) by Computer",
                                        "yAxis": {
                                            "isLogarithmic": false,
                                            "units": {
                                                "baseUnitType": "",
                                                "baseUnit": "",
                                                "displayUnit": ""
                                            },
                                            "customLabel": ""
                                        }
                                    },
                                    "List": {
                                        "Query": "Type:Perf  ((ObjectName:Memory AND CounterName:\"Available MBytes\" )) | measure percentile90(CounterValue) as AvgFreeMemory by Computer | sort AvgFreeMemory",
                                        "HideGraph": false,
                                        "enableSparklines": false,
                                        "operation": "Summary",
                                        "ColumnsTitle": {
                                            "Name": "Computer",
                                            "Value": "% Committed Bytes in Use"
                                        },
                                        "Color": "#0072c6",
                                        "thresholds": {
                                            "isEnabled": true,
                                            "values": [
                                                {
                                                    "name": "Error",
                                                    "threshold": "Default",
                                                    "color": "#ba141a",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": "2000",
                                                    "color": "#fcd116",
                                                    "isDefault": false
                                                },
                                                {
                                                    "name": "Healthy",
                                                    "threshold": "5000",
                                                    "color": "#007233",
                                                    "isDefault": false
                                                }
                                            ]
                                        },
                                        "NameDSVSeparator": "",
                                        "NavigationQuery": "Type:Perf  ((ObjectName:Memory AND CounterName:\"Available MBytes\" )) | measure percentile90(CounterValue) as AvgFreeMemory by Computer | sort AvgFreeMemory  | Display StackedBarChart"
                                    }
                                }
                            },
                            {
                                "Id": "SingleQueryDonutBuilderBladeV1",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Network Usage",
                                        "newGroup": true,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Header": {
                                        "Title": "Network traffic per computer",
                                        "Subtitle": ""
                                    },
                                    "Donut": {
                                        "Query": "Type:Perf (ObjectName=\"Network Interface\") (CounterName=\"Bytes Total/sec\")  | measure Avg(CounterValue) as BytesTotal by Computer",
                                        "CenterLegend": {
                                            "Text": "Total",
                                            "Operation": "Sum",
                                            "ArcsToSelect": []
                                        },
                                        "Options": {
                                            "colors": [
                                                "#00188f",
                                                "#0072c6",
                                                "#00bcf2"
                                            ],
                                            "valueColorMapping": []
                                        }
                                    },
                                    "List": {
                                        "Query": "Type:Perf (ObjectName=\"Network Interface\") (CounterName=\"Bytes Total/sec\") | measure Avg(CounterValue) as BytesTotal by Computer | Display StackedBarChart",
                                        "HideGraph": false,
                                        "enableSparklines": true,
                                        "operation": "Summary",
                                        "ColumnsTitle": {
                                            "Name": "Computer",
                                            "Value": "Packets Per Second"
                                        },
                                        "Color": "#0072c6",
                                        "thresholds": {
                                            "isEnabled": false,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": "60",
                                                    "color": "#fcd116",
                                                    "isDefault": false
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": "90",
                                                    "color": "#ba141a",
                                                    "isDefault": false
                                                }
                                            ]
                                        },
                                        "NameDSVSeparator": "",
                                        "NavigationQuery": "Type:Perf (ObjectName=\"Network Interface\") (CounterName=\"Bytes Total/sec\") | measure Avg(CounterValue) as BytesTotal by Computer | Display StackedBarChart"
                                    }
                                }
                            },
                            {
                                "Id": "LineChartCalloutStackedBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Network Usage",
                                        "newGroup": false
                                    },
                                    "charts": [
                                        {
                                            "Header": {
                                                "Title": "Packets Received over time",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "Type:Perf (ObjectName=\"Network Adapter\") (CounterName=\"Bytes Received/sec\")  | measure Avg(CounterValue) as PacketsReceived by Computer",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "",
                                                        "baseUnit": "",
                                                        "displayUnit": ""
                                                    },
                                                    "customLabel": ""
                                                }
                                            }
                                        },
                                        {
                                            "Header": {
                                                "Title": "Packets Sent over time",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "Type:Perf (ObjectName=\"Network Adapter\") (CounterName=\"Bytes Sent/sec\") | measure Avg(CounterValue) as PacketsSent by Computer",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "",
                                                        "baseUnit": "",
                                                        "displayUnit": ""
                                                    },
                                                    "customLabel": ""
                                                }
                                            }
                                        },
                                        {
                                            "Header": {
                                                "Title": "Packets per second over time",
                                                "Subtitle": ""
                                            },
                                            "LineChart": {
                                                "Query": "Type:Perf (ObjectName=\"Network Interface\") (CounterName=\"Bytes Total/sec\") | measure Avg(CounterValue) as BytesTotal by Computer",
                                                "yAxis": {
                                                    "isLogarithmic": false,
                                                    "units": {
                                                        "baseUnitType": "",
                                                        "baseUnit": "",
                                                        "displayUnit": ""
                                                    },
                                                    "customLabel": ""
                                                }
                                            }
                                        }
                                    ]
                                }
                            }
                        ],
                        "OverviewTile": {
                            "Id": "DoubleNumberBuilderTile",
                            "Type": "OverviewTile",
                            "Version": 0,
                            "Configuration": {
                                "TileOne": {
                                    "Legend": "Servers currently reporting to OMS",
                                    "Query": "MG:* | Measure Max(TimeGenerated) as LastData by Computer | where LastData>NOW-1HOURS"
                                },
                                "TileTwo": {
                                    "Legend": "Servers not currently reporting to OMS",
                                    "Query": "MG:* | Measure Max(TimeGenerated) as LastData by Computer | where LastData<NOW-1HOURS"
                                },
                                "Advanced": {
                                    "DataFlowVerification": {
                                        "Enabled": false,
                                        "Query": "*",
                                        "Message": ""
                                    }
                                }
                            },
                            "Navigation": [
                                {
                                    "Host": "OMSPortal",
                                    "Link": {
                                        "Type": "Builder",
                                        "Id": "Server Performance"
                                    }
                                },
                                {
                                    "Host": "Mobile",
                                    "Link": {
                                        "Type": "Builder",
                                        "Id": "Server Performance"
                                    }
                                },
                                {
                                    "Host": "Azure",
                                    "Link": {
                                        "Type": "Builder",
                                        "Id": "Server Performance"
                                    }
                                }
                            ]
                        }
                    }
                }
            ]
        }
    ]
}